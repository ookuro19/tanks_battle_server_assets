<root>
	<Interfaces>
		<Interface>	EntityCommon		</Interface>
	</Interfaces>

	<Properties>
		<roomKey>
			<Type>		SPACE_KEY			</Type>
			<Flags>		BASE				</Flags>
			<Default>		0				</Default>
		</roomKey>

		<mapNum>
			<Type>		INT32			</Type>
			<Flags>		BASE			</Flags>
			<Default>		0			</Default>
		</mapNum>

		<modeNum>
			<Type>		INT32			</Type>
			<Flags>		BASE			</Flags>
			<Default>		0			</Default>
		</modeNum>

		<matchCode>
			<Type>		INT32			</Type>
			<Flags>		BASE			</Flags>
			<Default>		0			</Default>
		</matchCode>

		<nameS>
			<Type>		UNICODE			</Type>
			<Flags>		ALL_CLIENTS			</Flags>
			<Persistent>		true				</Persistent>
			<DetailLevel>		MEDIUM			</DetailLevel>
		</nameS>

		<dbid>
			<Type>		DBID				</Type>
			<Flags>		CELL_PUBLIC			</Flags>
			<Default>		0				</Default>
		</dbid>

		<progress>
			<Type>		INT32			</Type>
			<Flags>		OTHER_CLIENTS		</Flags>
			<Default>		0			</Default>
		</progress>

		<loginState>
			<Type>		INT32			</Type>
			<Flags>		BASE		</Flags>
			<Default>		0				</Default>
		</loginState>

		<roomNo>
			<Type>		INT32			</Type>
			<Flags>		ALL_CLIENTS		</Flags>
			<Default>		0			</Default>
		</roomNo>
	</Properties>

	<ClientMethods>
		<onLoginState>
			<Arg>		INT32				</Arg>
		</onLoginState>

		<onMapModeChanged>
			<Arg>		INT32				</Arg>
		</onMapModeChanged>

		<onMatchingFinish>
			<Arg>		INT32				</Arg>
		</onMatchingFinish>

		<onLoadingFinish>
			<Arg>		INT32				</Arg>
		</onLoadingFinish>

		<onGetProps>
			<PROP_KEY>		INT32			</PROP_KEY>
			<Arg>			INT32			</Arg>
		</onGetProps>

		<onUseProp>
			<Arg>		ENTITY_ID		</Arg>
			<Arg>		ENTITY_ID		</Arg>
			<Arg>		INT32			</Arg>
		</onUseProp>

		<onPropResult>
			<Arg>		ENTITY_ID		</Arg>
			<Arg>		ENTITY_ID		</Arg>
			<Arg>		PROP_TYPE		</Arg>
			<Arg>		INT32			</Arg>
		</onPropResult>

		<onReachDestination>
			<Arg>		ENTITY_ID		</Arg>
			<Arg>		INT32			</Arg>
		</onReachDestination>

		<onTimerChanged>
			<Arg>		INT32			</Arg>
		</onTimerChanged>

		<onExitRoom>
			<Arg>		INT32			</Arg>
		</onExitRoom>
	</ClientMethods>

	<BaseMethods>
		<regStartMatching>
			<Exposed/>
			<Arg>		MODE_NUM					</Arg>
			<Arg>		MAP_NUM						</Arg>
			<Arg>		MATCH_CODE					</Arg>
		</regStartMatching>

		<createCell>
			<Arg>		ENTITYCALL					</Arg>
			<Arg>		SPACE_KEY					</Arg>
			<Arg>		INT32						</Arg>
		</createCell>

		<onMapModeChanged>
			<Arg>		MODE_NUM					</Arg>
			<Arg>		MAP_NUM						</Arg>
		</onMapModeChanged>

		<onMatchingFinish>
			<Arg>		INT32				</Arg>
		</onMatchingFinish>

		<onLoadingFinish>
			<Arg>		INT32				</Arg>
		</onLoadingFinish>

		<onGetProps>
			<Arg>		PROP_KEY		</Arg>
			<Arg>		INT32			</Arg>
		</onGetProps>

		<onReachDestination>
			<Arg>		ENTITY_ID		</Arg>
			<Arg>		INT32			</Arg>
		</onReachDestination>

		<onTimerChanged>
			<Arg>		INT32			</Arg>
		</onTimerChanged>
	</BaseMethods>

	<CellMethods>
		<regProgress>
			<Exposed/>
			<Arg>		INT32						</Arg>
		</regProgress>

		<regReachDestination>
			<Exposed/>
		</regReachDestination>

		<!-- reg 获取道具 -->
		<regGetProps>
			<Exposed/>
			<Arg>		INT32						</Arg>
		</regGetProps>

		<!-- reg 使用道具 -->
		<regUseProp>
			<Exposed/>
			<Arg>		ENTITY_ID					</Arg>
			<Arg>		INT32						</Arg>
		</regUseProp>

		<!-- reg 道具使用结果 -->
		<regPropResult>
			<Exposed/>
			<Arg>		ENTITY_ID					</Arg>
			<Arg>		INT32						</Arg>
		</regPropResult>

		<!-- 服务器结算技能回调 -->
		<onPropResult>
			<Arg>		ENTITY_ID				</Arg>
			<Arg>		PROP_TYPE				</Arg>
			<Arg>		INT32					</Arg>
		</onPropResult>
	</CellMethods>

</root>
