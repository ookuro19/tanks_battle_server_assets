<root>
	<Interfaces>
		<Interface>	EntityCommon		</Interface>
	</Interfaces>

	<Properties>
		<roomKey>
			<Type>		SPACE_KEY			</Type>
			<Flags>		BASE				</Flags>
			<Persistent>		true				</Persistent>
		</roomKey>

		<name>
			<Type>		UNICODE			</Type>
			<Flags>		ALL_CLIENTS			</Flags>
			<Persistent>		true				</Persistent>
			<DetailLevel>		MEDIUM			</DetailLevel>
		</name>

		<dbid>
			<Type>		DBID				</Type>
			<Flags>		CELL_PUBLIC			</Flags>
			<Default>		0				</Default>
		</dbid>

		<progress>
			<Type>		INT32			</Type>
			<Flags>		OTHER_CLIENTS		</Flags>
			<Default>		0				</Default>
		</progress>

		<loginState>
			<Type>		INT32			</Type>
			<Flags>		BASE		</Flags>
			<Default>		0				</Default>
		</loginState>

		<roomNo>
			<Type>		INT32			</Type>
			<Flags>		ALL_CLIENTS		</Flags>
			<Default>		0			</Default>
		</roomNo>
	</Properties>

	<ClientMethods>
		<onLoginState>
			<Arg>		INT32				</Arg>
		</onLoginState>

		<onMatchingFinish>
			<Arg>		INT32				</Arg>
		</onMatchingFinish>

		<onLoadingFinish>
			<Arg>		INT32				</Arg>
		</onLoadingFinish>

		<onGetProps>
			<Arg>		INT32			</Arg>
		</onGetProps>

		<onUseSkill>
			<Arg>		ENTITY_ID		</Arg>
			<Arg>		ENTITY_ID		</Arg>
			<Arg>		INT32			</Arg>
		</onUseSkill>

		<onSkillResult>
			<Arg>		ENTITY_ID		</Arg>
			<Arg>		ENTITY_ID		</Arg>
			<Arg>		INT32			</Arg>
		</onSkillResult>

		<onReachDestination>
			<Arg>		ENTITY_ID		</Arg>
			<Arg>		INT32			</Arg>
		</onReachDestination>

		<onTimerChanged>
			<Arg>		INT32			</Arg>
		</onTimerChanged>
	</ClientMethods>

	<BaseMethods>
		<startMatching>
			<Exposed/>
			<Arg>		MAP_NUM						</Arg>
			<Arg>		MODE_NUM					</Arg>
		</startMatching>

		<createCell>
			<Arg>		ENTITYCALL					</Arg>
			<Arg>		SPACE_KEY					</Arg>
			<Arg>		INT32						</Arg>
		</createCell>

		<matchingFinish>
		</matchingFinish>

		<onLoadingFinish>
			<Arg>		INT32				</Arg>
		</onLoadingFinish>
	</BaseMethods>

	<CellMethods>
		<regProgress>
			<Exposed/>
			<Arg>		INT32						</Arg>
		</regProgress>

		<loadingFinish>
		</loadingFinish>

		<reachDestination>
			<Exposed/>
		</reachDestination>

		<getProps>
			<Exposed/>
			<Arg>		INT32						</Arg>
		</getProps>

		<useSkill>
			<Exposed/>
			<Arg>		ENTITY_ID					</Arg>
			<Arg>		INT32						</Arg>
		</useSkill>

		<skillResult>
			<Exposed/>
			<Arg>		ENTITY_ID					</Arg>
			<Arg>		INT32						</Arg>
		</skillResult>
	</CellMethods>

</root>
